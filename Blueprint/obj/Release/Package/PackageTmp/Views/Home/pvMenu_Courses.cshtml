@model List<Blueprint.Models.CoursesItem>
@using Blueprint.Utils

@{
    var model = Model.Where(item => item.Status.Equals("A"));
}

@foreach (var item in model)
{
    <li><a href="@Url.Action("Course", "Home", new { title = Utility.EnterByDash(Utility.ConvertToUnsign(item.Title)) })">@item.Title</a></li>
}